<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>net5.0</TargetFramework>
    <PackageProjectUrl>https://github.com/CaiJinHao/netcore-api-template/tree/master/services/common/Common.NetCoreWebUtility</PackageProjectUrl>
    <RepositoryUrl>https://github.com/CaiJinHao/netcore-api-template</RepositoryUrl>
    <PackageReleaseNotes>This is a complete template project that contains the entire project dependent reference and use</PackageReleaseNotes>
    <PackageTags>C#,netcore,utility,netstandard2.0,netstandard2.1</PackageTags>
    <RepositoryType>git</RepositoryType>
    <PackageIcon>nugeticon.png</PackageIcon>
    <Description>This is an extended class library for the net core web API, including some component authorization, Swagger, and other references</Description>
    <Authors>CaiJinHao</Authors>
    <Company>CaiJinHao</Company>
    <Version>0.0.2</Version>
    <GeneratePackageOnBuild>false</GeneratePackageOnBuild>
  </PropertyGroup>

	<ItemGroup>
		<PackageReference Include="IdentityModel" Version="4.5.0" />
		<PackageReference Include="Microsoft.AspNetCore.Mvc.Core" Version="2.2.5" />
		<PackageReference Include="Microsoft.Extensions.Configuration.Json" Version="5.0.0" />
		<PackageReference Include="Swashbuckle.AspNetCore" Version="5.6.3" />
		<PackageReference Include="Swashbuckle.AspNetCore.Swagger" Version="5.6.3" />
		<PackageReference Include="Swashbuckle.AspNetCore.SwaggerGen" Version="5.6.3" />
		<PackageReference Include="System.ComponentModel.Annotations" Version="5.0.0" />
	</ItemGroup>

	<ItemGroup>
	  <ProjectReference Include="..\Common.Utility\Common.Utility.csproj" />
	</ItemGroup>

</Project>
