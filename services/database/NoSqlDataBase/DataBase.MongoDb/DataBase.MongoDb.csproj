<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
	  <TargetFrameworks>netstandard2.0;netstandard2.1</TargetFrameworks>
	  <Version>0.0.1</Version>
	  <AssemblyName>DataBase.MongoDb</AssemblyName>
	  <RootNamespace>DataBse</RootNamespace>
	  <Description>DataBse.MongoDb is a class library tool that connects to MySql database by using Dapper driver</Description>
	  <Authors>CaiJinHao</Authors>
	  <Owners>CaiJinHao</Owners>
	  <PackageTags>C#,utility,netstandard2.0,netstandard2.1</PackageTags>
	  <Title>Common.Utility: The class library tools</Title>
