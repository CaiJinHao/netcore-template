    <PackageReference Include="System.Text.Json" Version="4.7.2" />
  </ItemGroup>

  <ItemGroup>
    <ProjectReference Include="..\..\IDataBase\IDataBase.csproj" />
  </ItemGroup>

  <ItemGroup>
    <None Include="..\..\..\..\documents\images\nugeticon.png">
      <Pack>True</Pack>
      <PackagePath></PackagePath>
    </None>
  </ItemGroup>

